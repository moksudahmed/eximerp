<div class="">
  <h1>New factory</h1>

  <div class="body">
    <form class="" action="/factory/update" method="post">
      <input name="id" value="<%= element.id %>" hidden="true" required>
        <div class="">
          <label for="">address</label>
          <input type="text" name="address" value="<%= element.address %>" required="true">
        </div>
        <div class="">
          <label for="">factoryName</label>
          <input type="text" name="factoryName" value="<%= element.factoryName %>" required="true">
        </div>
        <div class="">
          <label for="">contactNo</label>
          <input type="text" name="contactNo" value="<%= element.contactNo %>" required="true">
        </div>
        <div class="">
          <label for="">emailAddress</label>
          <input type="text" name="emailAddress" value="<%= element.emailAddress %>" required="true">
        </div>
        <div class="">
          <label for="">factoryID</label>
          <input type="number" name="factoryID" value="<%= element.factoryID %>" required="false">
        </div>
        <div class="">
          <label for="">employeeID</label>
          <input type="number" name="employeeID" value="<%= element.employeeID %>" required="true">
        </div>
        <div class="">
          <label for="">warehouseID</label>
          <input type="number" name="warehouseID" value="<%= element.warehouseID %>" required="true">
        </div>
        <div class="">
          <label for="">organizationID</label>
          <input type="number" name="organizationID" value="<%= element.organizationID %>" required="false">
        </div>
        <div class="">
          <label for="">createdAt</label>
          <input type="" name="createdAt" value="<%= element.createdAt %>" required="true">
        </div>
        <div class="">
          <label for="">updatedAt</label>
          <input type="" name="updatedAt" value="<%= element.updatedAt %>" required="true">
        </div>
      <button type="submit">Update factory</button>
    </form>
    <br />
    <a href="/factory">Index</a>
  </div>
</div>