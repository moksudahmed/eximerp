<h1>client</h1>


<% if (result.length == 0) { %>
  <h4>
    0 client
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>clientType</th>
			<th>date</th>
			<th>clientID</th>
			<th>organizationID</th>
			<th>subSidiaryAccountID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.clientType %>
					</td>
					<td>
						<%= item.date %>
					</td>
					<td>
						<%= item.clientID %>
					</td>
					<td>
						<%= item.organizationID %>
					</td>
					<td>
						<%= item.subSidiaryAccountID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/client/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/client/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/client/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/client/create">New</a>