<h1>purchaserequestcart</h1>


<% if (result.length == 0) { %>
  <h4>
    0 purchaserequestcart
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>itemID</th>
			<th>name</th>
			<th>quantity</th>
			<th>weight</th>
			<th>purchaseRequestCartID</th>
			<th>purchaseRequestID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.itemID %>
					</td>
					<td>
						<%= item.name %>
					</td>
					<td>
						<%= item.quantity %>
					</td>
					<td>
						<%= item.weight %>
					</td>
					<td>
						<%= item.purchaseRequestCartID %>
					</td>
					<td>
						<%= item.purchaseRequestID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/purchaserequestcart/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/purchaserequestcart/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/purchaserequestcart/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/purchaserequestcart/create">New</a>