<div class="">
  <h1>New overtime</h1>

  <div class="body">
    <form class="" action="/overtime/update" method="post">
      <input name="id" value="<%= element.id %>" hidden="true" required>
        <div class="">
          <label for="">overTimeDate</label>
          <input type="date" name="overTimeDate" value="<%= element.overTimeDate %>" required="true">
        </div>
        <div class="">
          <label for="">fromTime</label>
          <input type="datetime" name="fromTime" value="<%= element.fromTime %>" required="true">
        </div>
        <div class="">
          <label for="">totalHour</label>
          <input type="number" name="totalHour" value="<%= element.totalHour %>" required="true">
        </div>
        <div class="">
          <label for="">toTime</label>
          <input type="datetime" name="toTime" value="<%= element.toTime %>" required="true">
        </div>
        <div class="">
          <label for="">overtimeID</label>
          <input type="number" name="overtimeID" value="<%= element.overtimeID %>" required="false">
        </div>
        <div class="">
          <label for="">employeeID</label>
          <input type="number" name="employeeID" value="<%= element.employeeID %>" required="true">
        </div>
      <button type="submit">Update overtime</button>
    </form>
    <br />
    <a href="/overtime">Index</a>
  </div>
</div>