<h1>vwCustomerOrder</h1>


<% if (result.length == 0) { %>
  <h4>
    0 vwCustomerOrder
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>clientAccountID</th>
			<th>accountName</th>
			<th>active</th>
			<th>suspended</th>
			<th>accountCreditLimit</th>
			<th>customerOrderID</th>
			<th>status</th>
			<th>orderDate</th>
			<th>discount</th>
			<th>orderTotal</th>
			<th>paymentDate</th>
			<th>amount</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.clientAccountID %>
					</td>
					<td>
						<%= item.accountName %>
					</td>
					<td>
						<%= item.active %>
					</td>
					<td>
						<%= item.suspended %>
					</td>
					<td>
						<%= item.accountCreditLimit %>
					</td>
					<td>
						<%= item.customerOrderID %>
					</td>
					<td>
						<%= item.status %>
					</td>
					<td>
						<%= item.orderDate %>
					</td>
					<td>
						<%= item.discount %>
					</td>
					<td>
						<%= item.orderTotal %>
					</td>
					<td>
						<%= item.paymentDate %>
					</td>
					<td>
						<%= item.amount %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/vwCustomerOrder/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/vwCustomerOrder/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/vwCustomerOrder/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/vwCustomerOrder/create">New</a>