<h1>commissioned</h1>


<% if (result.length == 0) { %>
  <h4>
    0 commissioned
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>commissionRate</th>
			<th>commissionedID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.commissionRate %>
					</td>
					<td>
						<%= item.commissionedID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/commissioned/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/commissioned/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/commissioned/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/commissioned/create">New</a>