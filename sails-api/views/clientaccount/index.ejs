<h1>clientaccount</h1>


<% if (result.length == 0) { %>
  <h4>
    0 clientaccount
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>accountName</th>
			<th>accountNo</th>
			<th>joiningDate</th>
			<th>active</th>
			<th>suspended</th>
			<th>accountCreditLimit</th>
			<th>remarks</th>
			<th>clientAccountID</th>
			<th>clientID</th>
			<th>organizationID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.accountName %>
					</td>
					<td>
						<%= item.accountNo %>
					</td>
					<td>
						<%= item.joiningDate %>
					</td>
					<td>
						<%= item.active %>
					</td>
					<td>
						<%= item.suspended %>
					</td>
					<td>
						<%= item.accountCreditLimit %>
					</td>
					<td>
						<%= item.remarks %>
					</td>
					<td>
						<%= item.clientAccountID %>
					</td>
					<td>
						<%= item.clientID %>
					</td>
					<td>
						<%= item.organizationID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/clientaccount/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/clientaccount/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/clientaccount/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/clientaccount/create">New</a>