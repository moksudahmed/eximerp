<h1>orderdetails</h1>


<% if (result.length == 0) { %>
  <h4>
    0 orderdetails
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>discount</th>
			<th>quantity</th>
			<th>unitPrice</th>
			<th>total</th>
			<th>orderDetailsID</th>
			<th>customerOrderID</th>
			<th>itemID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.discount %>
					</td>
					<td>
						<%= item.quantity %>
					</td>
					<td>
						<%= item.unitPrice %>
					</td>
					<td>
						<%= item.total %>
					</td>
					<td>
						<%= item.orderDetailsID %>
					</td>
					<td>
						<%= item.customerOrderID %>
					</td>
					<td>
						<%= item.itemID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/orderdetails/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/orderdetails/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/orderdetails/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/orderdetails/create">New</a>