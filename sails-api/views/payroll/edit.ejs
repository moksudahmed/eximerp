<div class="">
  <h1>New payroll</h1>

  <div class="body">
    <form class="" action="/payroll/update" method="post">
      <input name="id" value="<%= element.id %>" hidden="true" required>
        <div class="">
          <label for="">approve</label>
          <input type="date" name="approve" value="<%= element.approve %>" required="true">
        </div>
        <div class="">
          <label for="">payDate</label>
          <input type="date" name="payDate" value="<%= element.payDate %>" required="true">
        </div>
        <div class="">
          <label for="">payFrom</label>
          <input type="date" name="payFrom" value="<%= element.payFrom %>" required="true">
        </div>
        <div class="">
          <label for="">payTo</label>
          <input type="date" name="payTo" value="<%= element.payTo %>" required="true">
        </div>
        <div class="">
          <label for="">prepeareDate</label>
          <input type="date" name="prepeareDate" value="<%= element.prepeareDate %>" required="true">
        </div>
        <div class="">
          <label for="">status</label>
          <input type="text" name="status" value="<%= element.status %>" required="true">
        </div>
        <div class="">
          <label for="">tax</label>
          <input type="number" name="tax" value="<%= element.tax %>" required="true">
        </div>
        <div class="">
          <label for="">totalGrossPay</label>
          <input type="number" name="totalGrossPay" value="<%= element.totalGrossPay %>" required="true">
        </div>
        <div class="">
          <label for="">totalNetPay</label>
          <input type="number" name="totalNetPay" value="<%= element.totalNetPay %>" required="true">
        </div>
        <div class="">
          <label for="">chartOfAccountID</label>
          <input type="number" name="chartOfAccountID" value="<%= element.chartOfAccountID %>" required="true">
        </div>
        <div class="">
          <label for="">payrollID</label>
          <input type="number" name="payrollID" value="<%= element.payrollID %>" required="false">
        </div>
        <div class="">
          <label for="">employeeID</label>
          <input type="number" name="employeeID" value="<%= element.employeeID %>" required="true">
        </div>
      <button type="submit">Update payroll</button>
    </form>
    <br />
    <a href="/payroll">Index</a>
  </div>
</div>