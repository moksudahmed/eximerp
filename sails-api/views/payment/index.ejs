<h1>payment</h1>


<% if (result.length == 0) { %>
  <h4>
    0 payment
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>amount</th>
			<th>paymentDate</th>
			<th>paymentType</th>
			<th>status</th>
			<th>paymentID</th>
			<th>clientAccountID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.amount %>
					</td>
					<td>
						<%= item.paymentDate %>
					</td>
					<td>
						<%= item.paymentType %>
					</td>
					<td>
						<%= item.status %>
					</td>
					<td>
						<%= item.paymentID %>
					</td>
					<td>
						<%= item.clientAccountID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/payment/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/payment/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/payment/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/payment/create">New</a>