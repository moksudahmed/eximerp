<h1>wages</h1>


<% if (result.length == 0) { %>
  <h4>
    0 wages
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>grossIncome</th>
			<th>netIncome</th>
			<th>overtime</th>
			<th>status</th>
			<th>totalCommission</th>
			<th>totalHour</th>
			<th>wagesID</th>
			<th>payrollID</th>
			<th>salaryProfileID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.grossIncome %>
					</td>
					<td>
						<%= item.netIncome %>
					</td>
					<td>
						<%= item.overtime %>
					</td>
					<td>
						<%= item.status %>
					</td>
					<td>
						<%= item.totalCommission %>
					</td>
					<td>
						<%= item.totalHour %>
					</td>
					<td>
						<%= item.wagesID %>
					</td>
					<td>
						<%= item.payrollID %>
					</td>
					<td>
						<%= item.salaryProfileID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/wages/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/wages/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/wages/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/wages/create">New</a>