<h1>vwCorporateSupplier</h1>


<% if (result.length == 0) { %>
  <h4>
    0 vwCorporateSupplier
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>supplierID</th>
			<th>departmentID</th>
			<th>subSidiaryAccountID</th>
			<th>type</th>
			<th>organizationID</th>
			<th>companyName</th>
			<th>contactPersonName</th>
			<th>designation</th>
			<th>contactNo</th>
			<th>emailAddress</th>
			<th>website</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.supplierID %>
					</td>
					<td>
						<%= item.departmentID %>
					</td>
					<td>
						<%= item.subSidiaryAccountID %>
					</td>
					<td>
						<%= item.type %>
					</td>
					<td>
						<%= item.organizationID %>
					</td>
					<td>
						<%= item.companyName %>
					</td>
					<td>
						<%= item.contactPersonName %>
					</td>
					<td>
						<%= item.designation %>
					</td>
					<td>
						<%= item.contactNo %>
					</td>
					<td>
						<%= item.emailAddress %>
					</td>
					<td>
						<%= item.website %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/vwCorporateSupplier/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/vwCorporateSupplier/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/vwCorporateSupplier/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

