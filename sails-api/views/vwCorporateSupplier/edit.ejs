        <div class="">
          <label for="">supplierID</label>
          <input type="number" name="supplierID" value="<%= element.supplierID %>" required="true">
        </div>
        <div class="">
          <label for="">departmentID</label>
          <input type="number" name="departmentID" value="<%= element.departmentID %>" required="true">
        </div>
        <div class="">
          <label for="">subSidiaryAccountID</label>
          <input type="number" name="subSidiaryAccountID" value="<%= element.subSidiaryAccountID %>" required="true">
        </div>
        <div class="">
          <label for="">type</label>
          <input type="text" name="type" value="<%= element.type %>" required="true">
        </div>
        <div class="">
          <label for="">organizationID</label>
          <input type="number" name="organizationID" value="<%= element.organizationID %>" required="true">
        </div>
        <div class="">
          <label for="">companyName</label>
          <input type="text" name="companyName" value="<%= element.companyName %>" required="true">
        </div>
        <div class="">
          <label for="">contactPersonName</label>
          <input type="text" name="contactPersonName" value="<%= element.contactPersonName %>" required="true">
        </div>
        <div class="">
