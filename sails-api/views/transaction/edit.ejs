<div class="">
  <h1>New transaction</h1>

  <div class="body">
    <form class="" action="/transaction/update" method="post">
      <input name="id" value="<%= element.id %>" hidden="true" required>
        <div class="">
          <label for="">date</label>
          <input type="date" name="date" value="<%= element.date %>" required="true">
        </div>
        <div class="">
          <label for="">transactionTitle</label>
          <input type="text" name="transactionTitle" value="<%= element.transactionTitle %>" required="true">
        </div>
        <div class="">
          <label for="">transactionID</label>
          <input type="number" name="transactionID" value="<%= element.transactionID %>" required="false">
        </div>
        <div class="">
          <label for="">branchID</label>
          <input type="number" name="branchID" value="<%= element.branchID %>" required="true">
        </div>
        <div class="">
          <label for="">financialYearID</label>
          <input type="number" name="financialYearID" value="<%= element.financialYearID %>" required="false">
        </div>
        <div class="">
          <label for="">entryDateTime</label>
          <input type="datetime" name="entryDateTime" value="<%= element.entryDateTime %>" required="true">
        </div>
        <div class="">
          <label for="">userID</label>
          <input type="number" name="userID" value="<%= element.userID %>" required="false">
        </div>
        <div class="">
          <label for="">paymentID</label>
          <input type="number" name="paymentID" value="<%= element.paymentID %>" required="true">
        </div>
      <button type="submit">Update transaction</button>
    </form>
    <br />
    <a href="/transaction">Index</a>
  </div>
</div>