<div class="">
  <h1>New supplier</h1>

  <div class="body">
    <form class="" action="/supplier/update" method="post">
      <input name="id" value="<%= element.id %>" hidden="true" required>
        <div class="">
          <label for="">supplierID</label>
          <input type="number" name="supplierID" value="<%= element.supplierID %>" required="false">
        </div>
        <div class="">
          <label for="">departmentID</label>
          <input type="number" name="departmentID" value="<%= element.departmentID %>" required="true">
        </div>
        <div class="">
          <label for="">subSidiaryAccountID</label>
          <input type="number" name="subSidiaryAccountID" value="<%= element.subSidiaryAccountID %>" required="true">
        </div>
        <div class="">
          <label for="">type</label>
          <input type="text" name="type" value="<%= element.type %>" required="true">
        </div>
        <div class="">
          <label for="">category</label>
          <input type="text" name="category" value="<%= element.category %>" required="true">
        </div>
      <button type="submit">Update supplier</button>
    </form>
    <br />
    <a href="/supplier">Index</a>
  </div>
</div>