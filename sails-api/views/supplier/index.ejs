<h1>supplier</h1>


<% if (result.length == 0) { %>
  <h4>
    0 supplier
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>supplierID</th>
			<th>departmentID</th>
			<th>subSidiaryAccountID</th>
			<th>type</th>
			<th>category</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.supplierID %>
					</td>
					<td>
						<%= item.departmentID %>
					</td>
					<td>
						<%= item.subSidiaryAccountID %>
					</td>
					<td>
						<%= item.type %>
					</td>
					<td>
						<%= item.category %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/supplier/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/supplier/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/supplier/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/supplier/create">New</a>