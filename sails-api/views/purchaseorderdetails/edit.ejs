<div class="">
  <h1>New purchaseorderdetails</h1>

  <div class="body">
    <form class="" action="/purchaseorderdetails/update" method="post">
      <input name="id" value="<%= element.id %>" hidden="true" required>
        <div class="">
          <label for="">itemID</label>
          <input type="number" name="itemID" value="<%= element.itemID %>" required="true">
        </div>
        <div class="">
          <label for="">quantity</label>
          <input type="number" name="quantity" value="<%= element.quantity %>" required="true">
        </div>
        <div class="">
          <label for="">unitPrice</label>
          <input type="number" name="unitPrice" value="<%= element.unitPrice %>" required="true">
        </div>
        <div class="">
          <label for="">weight</label>
          <input type="number" name="weight" value="<%= element.weight %>" required="true">
        </div>
        <div class="">
          <label for="">purchaseOrderDetailsID</label>
          <input type="number" name="purchaseOrderDetailsID" value="<%= element.purchaseOrderDetailsID %>" required="false">
        </div>
        <div class="">
          <label for="">purchaseOrderID</label>
          <input type="number" name="purchaseOrderID" value="<%= element.purchaseOrderID %>" required="true">
        </div>
      <button type="submit">Update purchaseorderdetails</button>
    </form>
    <br />
    <a href="/purchaseorderdetails">Index</a>
  </div>
</div>