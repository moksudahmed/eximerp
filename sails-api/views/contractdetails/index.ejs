<h1>contractdetails</h1>


<% if (result.length == 0) { %>
  <h4>
    0 contractdetails
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>itemInfo</th>
			<th>quantity</th>
			<th>itemPrice</th>
			<th>contractDetailsID</th>
			<th>salesContractID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.itemInfo %>
					</td>
					<td>
						<%= item.quantity %>
					</td>
					<td>
						<%= item.itemPrice %>
					</td>
					<td>
						<%= item.contractDetailsID %>
					</td>
					<td>
						<%= item.salesContractID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/contractdetails/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/contractdetails/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/contractdetails/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/contractdetails/create">New</a>