<h1>customerorder</h1>


<% if (result.length == 0) { %>
  <h4>
    0 customerorder
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>discount</th>
			<th>orderDate</th>
			<th>receiveDate</th>
			<th>status</th>
			<th>orderTotal</th>
			<th>customerOrderID</th>
			<th>clientAccountID</th>
			<th>employeeID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.discount %>
					</td>
					<td>
						<%= item.orderDate %>
					</td>
					<td>
						<%= item.receiveDate %>
					</td>
					<td>
						<%= item.status %>
					</td>
					<td>
						<%= item.orderTotal %>
					</td>
					<td>
						<%= item.customerOrderID %>
					</td>
					<td>
						<%= item.clientAccountID %>
					</td>
					<td>
						<%= item.employeeID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/customerorder/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/customerorder/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/customerorder/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/customerorder/create">New</a>