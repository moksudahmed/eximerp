<h1>vwCustomerPaymentDue</h1>


<% if (result.length == 0) { %>
  <h4>
    0 vwCustomerPaymentDue
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>clientAccountID</th>
			<th>due</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.clientAccountID %>
					</td>
					<td>
						<%= item.due %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/vwCustomerPaymentDue/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/vwCustomerPaymentDue/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/vwCustomerPaymentDue/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/vwCustomerPaymentDue/create">New</a>