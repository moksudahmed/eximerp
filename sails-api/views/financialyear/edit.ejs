<div class="">
  <h1>New financialyear</h1>

  <div class="body">
    <form class="" action="/financialyear/update" method="post">
      <input name="id" value="<%= element.id %>" hidden="true" required>
        <div class="">
          <label for="">startDate</label>
          <input type="date" name="startDate" value="<%= element.startDate %>" required="true">
        </div>
        <div class="">
          <label for="">endDate</label>
          <input type="date" name="endDate" value="<%= element.endDate %>" required="true">
        </div>
        <div class="">
          <label for="">financialYearID</label>
          <input type="number" name="financialYearID" value="<%= element.financialYearID %>" required="false">
        </div>
      <button type="submit">Update financialyear</button>
    </form>
    <br />
    <a href="/financialyear">Index</a>
  </div>
</div>