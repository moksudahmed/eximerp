<h1>purchaserequest</h1>


<% if (result.length == 0) { %>
  <h4>
    0 purchaserequest
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>purchaseRequestID</th>
			<th>departmentID</th>
			<th>date</th>
			<th>status</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.purchaseRequestID %>
					</td>
					<td>
						<%= item.departmentID %>
					</td>
					<td>
						<%= item.date %>
					</td>
					<td>
						<%= item.status %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/purchaserequest/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/purchaserequest/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/purchaserequest/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/purchaserequest/create">New</a>