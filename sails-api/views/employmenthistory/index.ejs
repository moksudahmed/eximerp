<h1>employmenthistory</h1>


<% if (result.length == 0) { %>
  <h4>
    0 employmenthistory
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>designation</th>
			<th>organizationName</th>
			<th>address</th>
			<th>joiningDate</th>
			<th>leavingDate</th>
			<th>employmentHistoryID</th>
			<th>personID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.designation %>
					</td>
					<td>
						<%= item.organizationName %>
					</td>
					<td>
						<%= item.address %>
					</td>
					<td>
						<%= item.joiningDate %>
					</td>
					<td>
						<%= item.leavingDate %>
					</td>
					<td>
						<%= item.employmentHistoryID %>
					</td>
					<td>
						<%= item.personID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/employmenthistory/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/employmenthistory/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/employmenthistory/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/employmenthistory/create">New</a>