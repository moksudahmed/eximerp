<div class="">
  <h1>New users</h1>

  <div class="body">
    <form class="" action="/users/update" method="post">
      <input name="id" value="<%= element.id %>" hidden="true" required>
        <div class="">
          <label for="">id</label>
          <input type="number" name="id" value="<%= element.id %>" required="false">
        </div>
        <div class="">
          <label for="">name</label>
          <input type="text" name="name" value="<%= element.name %>" required="true">
        </div>
        <div class="">
          <label for="">username</label>
          <input type="text" name="username" value="<%= element.username %>" required="true">
        </div>
        <div class="">
          <label for="">email</label>
          <input type="text" name="email" value="<%= element.email %>" required="true">
        </div>
        <div class="">
          <label for="">password</label>
          <input type="text" name="password" value="<%= element.password %>" required="true">
        </div>
        <div class="">
          <label for="">createdAt</label>
          <input type="datetime" name="createdAt" value="<%= element.createdAt %>" required="false">
        </div>
        <div class="">
          <label for="">updatedAt</label>
          <input type="datetime" name="updatedAt" value="<%= element.updatedAt %>" required="false">
        </div>
      <button type="submit">Update users</button>
    </form>
    <br />
    <a href="/users">Index</a>
  </div>
</div>