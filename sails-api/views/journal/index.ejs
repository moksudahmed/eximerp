<h1>journal</h1>


<% if (result.length == 0) { %>
  <h4>
    0 journal
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>amount</th>
			<th>debitCredit</th>
			<th>invoiceNo</th>
			<th>journalID</th>
			<th>chartOfAccountID</th>
			<th>subSidiaryAccountID</th>
			<th>transactionID</th>
			<th>particulars</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.amount %>
					</td>
					<td>
						<%= item.debitCredit %>
					</td>
					<td>
						<%= item.invoiceNo %>
					</td>
					<td>
						<%= item.journalID %>
					</td>
					<td>
						<%= item.chartOfAccountID %>
					</td>
					<td>
						<%= item.subSidiaryAccountID %>
					</td>
					<td>
						<%= item.transactionID %>
					</td>
					<td>
						<%= item.particulars %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/journal/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/journal/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/journal/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/journal/create">New</a>