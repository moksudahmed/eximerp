<h1>timesheet</h1>


<% if (result.length == 0) { %>
  <h4>
    0 timesheet
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>attendanceDate</th>
			<th>present</th>
			<th>timesIn</th>
			<th>timesOut</th>
			<th>timesheetID</th>
			<th>employeeID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.attendanceDate %>
					</td>
					<td>
						<%= item.present %>
					</td>
					<td>
						<%= item.timesIn %>
					</td>
					<td>
						<%= item.timesOut %>
					</td>
					<td>
						<%= item.timesheetID %>
					</td>
					<td>
						<%= item.employeeID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/timesheet/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/timesheet/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/timesheet/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/timesheet/create">New</a>