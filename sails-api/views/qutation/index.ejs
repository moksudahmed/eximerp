<h1>qutation</h1>


<% if (result.length == 0) { %>
  <h4>
    0 qutation
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>itemID</th>
			<th>quantity</th>
			<th>price</th>
			<th>approved</th>
			<th>qutationID</th>
			<th>purchaseRequestID</th>
			<th>supplierID</th>
			<th>purchaseRequestCartID</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.itemID %>
					</td>
					<td>
						<%= item.quantity %>
					</td>
					<td>
						<%= item.price %>
					</td>
					<td>
						<%= item.approved %>
					</td>
					<td>
						<%= item.qutationID %>
					</td>
					<td>
						<%= item.purchaseRequestID %>
					</td>
					<td>
						<%= item.supplierID %>
					</td>
					<td>
						<%= item.purchaseRequestCartID %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/qutation/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/qutation/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/qutation/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

<br>
<a href="/qutation/create">New</a>