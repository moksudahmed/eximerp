<h1>vwIndividualSupplier</h1>


<% if (result.length == 0) { %>
  <h4>
    0 vwIndividualSupplier
  </h4>
<% } else { %>
<table>
	<thead>
    <th>#</th>
		<th>id</th>
			<th>supplierID</th>
			<th>departmentID</th>
			<th>subSidiaryAccountID</th>
			<th>type</th>
			<th>organizationID</th>
			<th>title</th>
			<th>firstName</th>
			<th>lastName</th>
			<th>dateOfBirth</th>
			<th>gender</th>
			<th>emailAddress</th>
			<th>mobileNo</th>
			<th>maritalStatus</th>

		<th>created at</th>
		<th>updated at</th>
		<th colspan="3"></th>
	</thead>

	<tbody>
		<% result.forEach((item, i) => { %>
			<tr>
				<td>
					<%= (i + 1) %>
				</td>
				<td>
					<%= item.id %>
				</td>
					<td>
						<%= item.supplierID %>
					</td>
					<td>
						<%= item.departmentID %>
					</td>
					<td>
						<%= item.subSidiaryAccountID %>
					</td>
					<td>
						<%= item.type %>
					</td>
					<td>
						<%= item.organizationID %>
					</td>
					<td>
						<%= item.title %>
					</td>
					<td>
						<%= item.firstName %>
					</td>
					<td>
						<%= item.lastName %>
					</td>
					<td>
						<%= item.dateOfBirth %>
					</td>
					<td>
						<%= item.gender %>
					</td>
					<td>
						<%= item.emailAddress %>
					</td>
					<td>
						<%= item.mobileNo %>
					</td>
					<td>
						<%= item.maritalStatus %>
					</td>
				<td>
					<%= item.createdAt %>
				</td>
				<td>
					<%= item.updatedAt %>
				</td>
				<td>
					<a href="/vwIndividualSupplier/show/<%= item.id %>">Show</a>
				</td>
				<td>
					<a href="/vwIndividualSupplier/delete/<%= item.id %>">Delete</a>
				</td>
        <td>
					<a href="/vwIndividualSupplier/edit/<%= item.id %>">Edit</a>
				</td>
			</tr>
			<% }) %>
	</tbody>
</table>

<% } %>

